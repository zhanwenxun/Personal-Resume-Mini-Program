

.homePage image {
  width: 100%;
  height: 100%;
}

.titleStyle {
    width: 50%;
    height: 350rpx;
    line-height: 350rpx;
    overflow: hidden;
    border-radius: 50%;
    box-shadow: 0 0 50rpx #000;
    color: #fff;
    font-size: 60rpx;
    margin-left: 25%;
    margin-bottom: 80rpx;
    margin-top: 80rpx;
    text-align: center;
    position: relative;
}

.imageStyle {
    width: 100%;
    height:100%;
}
.userNameStyle {
    width: 100%;
    height: 100%;
    overflow: hidden;
    text-align: center;
    position: absolute;
    top: 0;
    white-space: nowrap;
}

.homePage button {
    width: 50%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    box-shadow: 10rpx 10rpx 10rpx #989898;
    text-shadow: 0 0 10rpx #989898;
    color: #2F2F2F;
    margin-top: 30rpx;
    border: 1px solid #ABABAB;
    background: #fff;
    border-radius: 30rpx; 
}
.homePage button::after{
    border: none;
    border-radius: 30rpx;
}